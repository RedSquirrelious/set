{
    "name": "Set",
    "version": "1.0.0",
    "description": "Set Practice Game",
    "scripts": {
        "build": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
        "test": "jest",
        "serve": "cross-env NODE_ENV=production babel-node srcServer.js",
        "start": "cross-env NODE_ENV=dev babel-node srcServer.js --config=webpack.config.dev.js"
    },
    "author": "Sasha Pierson",
    "license": "MIT",
    "dependencies": {
        "dotenv": "6.2.0",
        "webpack-merge": "4.2.1"
    },
    "devDependencies": {
        "@babel/cli": "7.2.3",
        "@babel/core": "7.2.2",
        "@babel/node": "7.2.2",
        "@babel/plugin-proposal-class-properties": "7.2.3",
        "@babel/plugin-proposal-object-rest-spread": "7.2.0",
        "@babel/plugin-transform-async-to-generator": "7.2.0",
        "@babel/plugin-transform-modules-umd": "7.2.0",
        "@babel/plugin-transform-runtime": "7.2.0",
        "@babel/preset-env": "7.2.3",
        "@babel/preset-react": "7.0.0",
        "babel-jest": "23.6.0",
        "babel-loader": "8.0.5",
        "core-js": "2.6.2",
        "cross-env": "5.2.0",
        "css-loader": "2.1.0",
        "file-loader": "3.0.1",
        "jest": "23.6.0",
        "jest-mock": "23.2.0",
        "material-ui": "1.0.0-beta.28",
        "prop-types": "15.6.2",
        "react": "16.7.0",
        "react-dom": "16.7.0",
        "react-hot-loader": "4.6.3",
        "react-redux": "6.0.0",
        "react-test-renderer": "16.7.0",
        "redux": "4.0.1",
        "redux-thunk": "2.3.0",
        "style-loader": "0.2.1",
        "url-loader": "1.1.2",
        "webpack": "4.28.4",
        "webpack-cli": "3.2.1",
        "webpack-dev-middleware": "3.5.0",
        "webpack-dev-server": "3.1.14",
        "webpack-hot-middleware": "2.24.3",
        "webpack-node-externals": "1.7.2"
    },
    "repository": {
        "type": "git",
        "url": ""
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "jsx"
        ],
        "moduleDirectories": [
            "node_modules"
        ],
        "moduleNameMapper": {
            "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
            "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
        }
    }
}
